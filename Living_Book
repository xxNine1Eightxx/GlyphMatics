Œ©goŒ£‚àû	HTTP GET with error check	
package main
import (
  "fmt"
  "net/http"
)
func main(){
  resp, err := http.Get("https://example.com")
  if err != nil { panic(err) }
  fmt.Println(resp.Status)
}
