import asyncio, aiohttp

async def fetch(url):
    async with aiohttp.ClientSession() as s:
        async with s.get(url) as r:
            return await r.text()

async def main():
    txt = await fetch('https://example.com')
    print(len(txt))

if __name__ == '__main__':
    asyncio.run(main())
